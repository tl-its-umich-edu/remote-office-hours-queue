{
	"compilerOptions": {
		"target": "es6",
        "moduleResolution": "node",
        "lib": ["es6", "dom"],
		"strict": true,
        "esModuleInterop": true,
        "jsx": "react",
		"skipLibCheck": true,
		"sourceMap": true,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"assets/src"
	],
	"exclude": [
		"assets/dist",
		"node_modules",
		"staticfiles"
	]
}
